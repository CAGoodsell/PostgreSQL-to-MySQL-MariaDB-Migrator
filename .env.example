# PostgreSQL Source Database Configuration
PG_HOST=localhost
PG_PORT=5432
PG_DATABASE=your_postgresql_database
PG_USERNAME=your_postgresql_username
PG_PASSWORD=your_postgresql_password
PG_SCHEMA=your_schema_name

# PostgreSQL SSH Tunnel Configuration (optional)
# Set PG_SSH_ENABLED=true to enable SSH tunneling for PostgreSQL connection
PG_SSH_ENABLED=false
PG_SSH_HOST=
PG_SSH_PORT=22
PG_SSH_USER=
PG_SSH_KEY_PATH=
PG_SSH_PASSWORD=
PG_SSH_REMOTE_HOST=localhost
PG_SSH_REMOTE_PORT=5432
PG_SSH_LOCAL_PORT=0

# MySQL/MariaDB Target Database Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=your_mysql_database
MYSQL_USERNAME=your_mysql_username
MYSQL_PASSWORD=your_mysql_password

# MySQL/MariaDB SSH Tunnel Configuration (optional)
# Set MYSQL_SSH_ENABLED=true to enable SSH tunneling for MySQL/MariaDB connection
MYSQL_SSH_ENABLED=false
MYSQL_SSH_HOST=
MYSQL_SSH_PORT=22
MYSQL_SSH_USER=
MYSQL_SSH_KEY_PATH=
MYSQL_SSH_PASSWORD=
MYSQL_SSH_REMOTE_HOST=localhost
MYSQL_SSH_REMOTE_PORT=3306
MYSQL_SSH_LOCAL_PORT=0

# Migration Settings
MIGRATION_CHUNK_SIZE=10000
MIGRATION_LARGE_CHUNK_SIZE=50000
MIGRATION_LARGE_TABLE_THRESHOLD_MB=1000
MIGRATION_PARALLEL_WORKERS=4
MIGRATION_CHECKPOINT_INTERVAL=100
MIGRATION_SKIP_INDEXES=false
MIGRATION_TABLES_INCLUDE=
MIGRATION_TABLES_EXCLUDE=
MIGRATION_AFTER_DATE=
MIGRATION_BEFORE_DATE=
MIGRATION_DATE_COLUMN=
